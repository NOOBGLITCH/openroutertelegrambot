{
  "env": {
    "SERVERLESS": "true"
  },
  "routes": [
    {
      "src": "/",
      "dest": "/api/root.py"
    },
    {
      "src": "/api/bot/.*",
      "dest": "/api/bot.py"
    },
    {
      "src": "/updateWebhooks",
      "dest": "/api/webhooks.py"
    },
    {
      "src": "/api",
      "dest": "/",
      "status": 308
    },
    {
      "src": "/api/(?!bot).*",
      "dest": "/",
      "status": 308
    }
  ],
  "functions": {
    "api/**/*.py": {
      "runtime": "python@3.9"
    }
  }
}
